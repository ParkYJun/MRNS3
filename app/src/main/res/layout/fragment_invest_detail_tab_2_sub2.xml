<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="도로명"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scRoadName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="인접소재지\n지번"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scNearJibun"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="인접소재지\n도로명"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scNearRoadName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:layout_gravity="center_vertical|right"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="고시번호"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scGosiNum"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="고시일자"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scGosiDate"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:layout_gravity="center_vertical|right"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n"
        android:visibility="gone">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="ksp 파일"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scKsp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="교차점\n유무"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scCrossTF"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n"
        android:visibility="gone">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="성과표파일"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scScoreFile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
    </LinearLayout>



</LinearLayout>