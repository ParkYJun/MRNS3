<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="도면번호"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scPrintNum"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="측정점 위치설명도"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2.3"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"
                android:visibility="invisible"/>

            <!--<TextView-->
                <!--android:id="@+id/scWhere"-->
                <!--android:layout_width="0dp"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_weight="5"-->
                <!--android:layout_gravity="center_vertical"-->
                <!--android:gravity="center"-->
                <!--android:textSize="@dimen/invert_text_content_ss_size"-->
                <!--android:textColor="@color/uploadDetailValue"-->
                <!--android:background="@drawable/edittext_n"/>-->
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="300dp"
        android:background="@drawable/down_body_border_n">
        <ImageView
            android:id="@+id/scWhere"
            android:layout_width="match_parent"
            android:layout_height="300dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="위치약도"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"
                android:visibility="invisible"/>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/detail_13"
        android:layout_width="fill_parent"
        android:layout_height="300dp"
        android:background="@drawable/down_body_border_n">
        <ImageView
            android:id="@+id/scMap"
            android:layout_width="match_parent"
            android:layout_height="300dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="지사"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scBranch"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="소관청"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scSmallOrg"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:layout_gravity="center_vertical|right"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="행정구역"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scOrgSector"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_s_margin"
        android:paddingTop="@dimen/activity_s_margin"
        android:background="@drawable/down_body_border_n">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="지번"
                android:textSize="@dimen/invert_text_header_ss_size"
                android:textColor="@color/uploadDetail"/>
            <TextView
                android:id="@+id/scJibun"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:textSize="@dimen/invert_text_content_ss_size"
                android:textColor="@color/uploadDetailValue"
                android:background="@drawable/edittext_n"/>
        </LinearLayout>
    </LinearLayout>





</LinearLayout>