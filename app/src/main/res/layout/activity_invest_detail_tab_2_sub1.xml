<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/detail_ten"
        android:layout_width="fill_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/detail_nine"
        android:background="@drawable/down_body_border_n">

        <TextView
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="left"
            android:paddingLeft="16dp"
            android:text="도면번호"
            android:textSize="14dp"
            android:textColor="@color/uploadDetail"/>

        <TextView
            android:id="@+id/scPrintNum"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:text=""
            android:textSize="15dp"
            android:textColor="@color/uploadDetailValue"
            android:background="@drawable/edittext_n"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/detail_eleven"
        android:layout_width="fill_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/detail_ten"
        android:background="@drawable/down_body_border_n">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="left"
            android:paddingLeft="16dp"
            android:text="측정점 위치설명도"
            android:textSize="14dp"
            android:textColor="@color/uploadDetail"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/detail_16"
        android:layout_width="fill_parent"
        android:layout_height="300dp"
        android:layout_below="@+id/detail_twelve"
        android:background="@drawable/down_body_border_n">

        <ImageView
            android:id="@+id/scWhere"
            android:layout_width="match_parent"
            android:layout_height="300dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/detail_twelve"
        android:layout_width="fill_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/detail_eleven"
        android:background="@drawable/down_body_border_n">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="left"
            android:paddingLeft="16dp"
            android:text="위치약도"
            android:textSize="14dp"
            android:textColor="@color/uploadDetail"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/detail_13"
        android:layout_width="fill_parent"
        android:layout_height="300dp"
        android:layout_below="@+id/detail_twelve"
        android:background="@drawable/down_body_border_n">

        <ImageView
            android:id="@+id/scMap"
            android:layout_width="match_parent"
            android:layout_height="300dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/detail_14"
        android:layout_width="fill_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/detail_13"
        android:background="@drawable/down_body_border_n">

        <TextView
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="left"
            android:paddingLeft="16dp"
            android:text="지사"
            android:textSize="14dp"
            android:textColor="@color/uploadDetail"/>

        <TextView
            android:id="@+id/scBranch"
            android:layout_width="125dp"
            android:layout_height="42dp"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:text=""
            android:textSize="15dp"
            android:textColor="@color/uploadDetailValue"
            android:background="@drawable/edittext_n"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:paddingRight="12dp"
            android:text="소관청"
            android:textSize="14dp"
            android:textColor="@color/uploadDetail"/>

        <TextView
            android:id="@+id/scSmallOrg"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_gravity="center_vertical|right"
            android:gravity="center"
            android:layout_marginRight="16dp"
            android:textSize="15dp"
            android:textColor="@color/uploadDetailValue"
            android:background="@drawable/edittext_n"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/detail_15"
        android:layout_width="fill_parent"
        android:layout_height="66dp"
        android:layout_below="@+id/detail_14"
        android:background="@drawable/down_body_border_n">

        <TextView
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="left"
            android:paddingLeft="16dp"
            android:text="행정구역"
            android:textSize="14dp"
            android:textColor="@color/uploadDetail"/>

        <TextView
            android:id="@+id/scOrgSector"
            android:layout_width="125dp"
            android:layout_height="42dp"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:text=""
            android:textSize="15dp"
            android:textColor="@color/uploadDetailValue"
            android:background="@drawable/edittext_n"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:paddingRight="12dp"
            android:text="지번"
            android:textSize="14dp"
            android:textColor="@color/uploadDetail"/>

        <TextView
            android:id="@+id/scJibun"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_gravity="center_vertical|right"
            android:gravity="center"
            android:layout_marginRight="16dp"
            android:textSize="15dp"
            android:textColor="@color/uploadDetailValue"
            android:background="@drawable/edittext_n"/>
    </LinearLayout>



</LinearLayout>